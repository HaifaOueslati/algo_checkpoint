ALGORITHM chekpoint
VAR
characters: INTEGER 
word: INTEGER 
vowel: INTEGER 
str : STRING
space : INTEGER
BEGIN
word :=0
space:=0
vowel:=0
for characters from 0 to length(str)-1 do 
IF (str[characters]:=" ")THEN
space := space+1
END_IF
IF (str[characters] in ("a","e","o","u","i" ) THEN
vowel:= vowel+1
END_IF
END_FOR
word:= space+1
write(characters);
write(word);
write(vowel);

END
