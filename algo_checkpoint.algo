ALGORITHM chekpoint
VAR
characters: INTEGER :=0
word: INTEGER :=0
vowel: INTEGER :=0
str : STRING
BEGIN
for characters from 0 to length(str)-1 do 
IF (str[characters]:= " ")THEN
word++
END_IF
IF (str[characters] in ["a","e","o","u","i"])THEN
vowel++
END_IF
characters++
END_FOR
word++
write(characters);
write(word);
write(vowel);

END